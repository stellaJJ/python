def show_exchange(amount):
  print('잔돈',amount,'원')

  n500=amount//500
  amount%=500
  n100=amount//100
  amount%=100
  n50=amount//50
  amount%=50
  n10=amount//10
  amount%=10

  print('500원',n500,'개')
  print('100원',n100,'개')
  print('50원',n50,'개')
  print('10원',n10,'개')

total_cost=int(input('총금액?'))
payment=int(input('지불한금액?'))
exchange=payment-total_cost
show_exchange(exchange)